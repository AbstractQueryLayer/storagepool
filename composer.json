{
    "name": "ifcastle/aql-storagepool",
    "description": "Storage pooling and connection management for AQL",
    "keywords": [
        "aql",
        "query",
        "language",
        "storage"
    ],
    "type": "ifcastle-aql-package",
    "license": "MIT",
    "authors": [
        {
            "name": "Edmond Dantes",
            "email": "edmondifthen@proton.me"
        }
    ],
    "require": {
        "php": ">=8.3"
    },
    "autoload": {
        "psr-4": {
            "IfCastle\\AQL\\StoragePool\\": [
                "src/"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IfCastle\\AQL\\StoragePool\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.2",
        "rector/rector": "^1.2"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.8.x-dev"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit ./tests"
    }
}